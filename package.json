{
  "name": "knex-tablecleaner",
  "version": "4.1.0",
  "license": "MIT",
  "description": "Knex Tablecleaner",
  "maintainers": [
    {
      "name": "Igor Savin",
      "email": "kibertoad@gmail.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/kibertoad/knex-tablecleaner.git"
  },
  "scripts": {
    "coveralls": "nyc report --reporter=lcov",
    "test": "mocha test/**/*.spec.js",
    "test:coverage": "nyc mocha --opts \"./mocha.opts\" test",
    "test:ci": "npm run eslint && nyc mocha --opts \"./mocha.opts\" test",
    "prettier": "prettier --write \"{src,test}/**/*.{js,ts}\"",
    "lint": "eslint --format codeframe \"src/**/*.js\" \"test/**/*.js\"",
    "db:start": "docker-compose -f docker-compose.yml up --build -d mysql postgres mssql; docker-compose -f docker-compose.yml up waitmssql waitmysql waitpostgres",
    "db:stop": "docker-compose -f docker-compose.yml down"
  },
  "main": "index.js",
  "types": "index.d.ts",
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^7.16.0",
    "eslint-plugin-prettier": "^3.3.0",
    "knex": "^0.19.5",
    "mocha": "^6.2.3",
    "mssql": "^6.3.0",
    "mysql": "^2.18.1",
    "nyc": "^14.1.1",
    "pg": "^8.5.1",
    "prettier": "^2.2.1",
    "sinon": "^7.5.0",
    "sqlite3": "^4.2.0"
  },
  "dependencies": {
    "async": "^2.6.3"
  },
  "nyc": {
    "description": "test coverage",
    "include": [
      "src/**"
    ],
    "reporter": [
      "text"
    ],
    "cache": true,
    "all": true,
    "check-coverage": true,
    "lines": 85,
    "statements": 85,
    "functions": 100,
    "branches": 70
  },
  "keywords": [
    "postgresql",
    "mysql",
    "sqlite",
    "oracle",
    "mssql",
    "clean",
    "clear",
    "delete",
    "tables",
    "db"
  ],
  "files": [
    "bin/*",
    "lib/*",
    "lib/*",
    "scripts/*",
    "types/index.d.ts",
    "types/result.d.ts",
    "types/tables.d.ts",
    "CHANGELOG.md",
    "CONTRIBUTING.md",
    "knex.js",
    "LICENSE",
    "README.md",
    "UPGRADING.md"
  ]
}
