{
  "name": "knex-tablecleaner",
  "version": "5.0.0",
  "license": "MIT",
  "description": "Library for sequentially deleting all rows from a given list of DB tables.",
  "maintainers": [
    {
      "name": "Igor Savin",
      "email": "kibertoad@gmail.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/kibertoad/knex-tablecleaner.git"
  },
  "scripts": {
    "coveralls": "nyc report --reporter=lcov",
    "test": "mocha test/**/*.spec.js",
    "test:coverage": "nyc mocha test",
    "prettier": "prettier --write \"{src,test}/**/*.{js,ts}\"",
    "lint": "eslint --format codeframe \"src/**/*.js\" \"test/**/*.js\"",
    "db:start": "docker-compose --file docker-compose.yml up --build -d mysql postgres mssql && docker-compose --file docker-compose.yml up waitmssql waitmysql waitpostgres",
    "db:stop": "docker-compose --file docker-compose.yml down"
  },
  "main": "index.js",
  "types": "index.d.ts",
  "devDependencies": {
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "knex": "^0.95.11",
    "mocha": "^9.1.1",
    "mssql": "^7.2.1",
    "mysql": "^2.18.1",
    "nyc": "^15.1.0",
    "pg": "^8.7.1",
    "prettier": "^2.4.1",
    "sinon": "^11.1.2",
    "sqlite3": "^5.0.2"
  },
  "dependencies": {
    "async": "^3.2.1"
  },
  "nyc": {
    "description": "test coverage",
    "include": [
      "src/**"
    ],
    "reporter": [
      "text"
    ],
    "cache": true,
    "all": true,
    "check-coverage": true,
    "statements": 100,
    "branches": 100,
    "lines": 100,
    "functions": 100
  },
  "keywords": [
    "postgresql",
    "mysql",
    "sqlite",
    "oracle",
    "mssql",
    "clean",
    "clear",
    "delete",
    "tables",
    "db"
  ],
  "files": [
    "src/*",
    "index.d.ts",
    "index.js",
    "LICENSE",
    "README.md"
  ]
}
