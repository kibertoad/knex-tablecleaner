{
  "name": "knex-tablecleaner",
  "version": "3.1.0",
  "description": "Knex Tablecleaner",
  "maintainers": [
    {
      "name": "Igor Savin",
      "email": "kibertoad@gmail.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/kibertoad/knex-tablecleaner.git"
  },
  "scripts": {
    "test": "mocha test/**/*.spec.js",
    "test-with-coverage": "nyc mocha --opts \"./mocha.opts\" test",
    "test-travis": "npm run eslint && nyc mocha --opts \"./mocha.opts\" test",
    "prettier": "prettier --write \"{src,test}/**/*.{js,ts}\"",
    "eslint": "eslint --format codeframe \"src/**/*.js\" \"test/**/*.js\""
  },
  "main": "index.js",
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^5.7.0",
    "eslint-plugin-prettier": "^3.0.0",
    "knex": "^0.15.2",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "prettier": "^1.14.3",
    "sinon": "^7.1.0",
    "sqlite3": "^4.0.3"
  },
  "dependencies": {
    "async": "^2.6.1"
  },
  "nyc": {
    "description": "test coverage",
    "exclude": [
      "node_modules",
      "test",
      "build",
      "examples"
    ],
    "reporter": [
      "html"
    ],
    "cache": true,
    "all": true,
    "report-dir": "./build/reports/coverage",
    "check-coverage": true,
    "lines": 80,
    "statements": 80,
    "functions": 100,
    "branches": 70
  }
}
